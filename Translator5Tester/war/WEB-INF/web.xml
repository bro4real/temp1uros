<?xml version="1.0" encoding="utf-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xmlns="http://java.sun.com/xml/ns/javaee"
xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
	<servlet>
		<servlet-name>Translator5Tester</servlet-name>
		<servlet-class>com.brothers4real.translator5tester.Translator5TesterServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Translator5Tester</servlet-name>
		<url-pattern>/translator5tester</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>RuTranslator</servlet-name>
		<servlet-class>com.brothers4real.translator5tester.RuTranslator</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>RuTranslator</servlet-name>
		<url-pattern>/translate</url-pattern>
	</servlet-mapping>
	<welcome-file-list>
		<welcome-file>rutrans.jsp</welcome-file>
	</welcome-file-list>
</web-app>
